<?xml version="1.0" encoding="UTF-8"?>
<STTProject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ogc="http://www.opengis.net/ogc" xmlns:sld="http://www.opengis.net/sld" xmlns:gml="http://www.opengis.net/gml" xmlns:swe="http://www.opengis.net/swe" xmlns:stt="http://vast.uah.edu/stt">
   <!-- Global metadata about the project -->
   <Identification>
      <name>Alabama Roads Test Project</name>
      <author>Alex Robin</author>
      <dateCreated>2005-11-02T09:03:00</dateCreated>
      <description>Alabama Highways and Interstates</description>
   </Identification>
   <!-- List of scenes (separate 2d or 3D view windows) -->
   <SceneList>
      <member>
         <Scene id="SCENE_01">
            <name>3D ECF View</name>
            <renderer>OPENGL</renderer>
            <view>
               <ViewSettings>
                  <backgroundColor>#FFFFFF</backgroundColor>
                  <viewWidth>1000</viewWidth>
                  <viewHeight>800</viewHeight>
                  <cameraMode>ORTHO</cameraMode>
                  <cameraPos>0 0 10</cameraPos>
                  <targetPos>0 0 0</targetPos>
                  <upDirection>0 1 0</upDirection>
                  <orthoWidth>4</orthoWidth>
                  <orthoHeight>1.67</orthoHeight>
                  <nearClip>0</nearClip>
                  <farClip>1e7</farClip>
                  <zFudgeFactor>1000</zFudgeFactor>
               </ViewSettings>
            </view>
            <contents>
               <DataList>
                  <name>MyData</name>
                  <member>
                     <DataList>
                        <name>Geographic Data Sets</name>
                        <member>
                           <DataItem enabled="true">
                              <name>AL State Boundaries</name>
                              <dataProvider xlink:href="#AL_STATE"/>
                              <style>
                                 <sld:LineSymbolizer enabled="true">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-breaks">
                                          <ogc:PropertyName>lineData/segmentSize</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>
                                    </sld:Geometry>
                                    <sld:Stroke>
                                       <sld:CssParameter name="stroke">#000000</sld:CssParameter>
                                       <sld:CssParameter name="stroke-width">2</sld:CssParameter>
                                    </sld:Stroke>
                                 </sld:LineSymbolizer>
                              </style>
                           </DataItem>
                        </member>
						      <member>
						         <DataItem enabled="true">
                              <name>AL County Boundaries</name>
						            <dataProvider xlink:href="#AL_COUNTIES"/>
                              <style>
                                 <sld:LineSymbolizer enabled="true">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-breaks">
                                          <ogc:PropertyName>lineData/segmentSize</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>
                                    </sld:Geometry>
                                    <sld:Stroke>
                                       <sld:CssParameter name="stroke">#222222</sld:CssParameter>
                                       <sld:CssParameter name="stroke-width">1</sld:CssParameter>
                                    </sld:Stroke>
                                 </sld:LineSymbolizer>
                              </style>
                           </DataItem>
                        </member>
                        <member>
                           <DataItem enabled="true">
                              <name>AL Interstates</name>
                              <dataProvider xlink:href="#AL_INTERSTATES"/>
                              <style>
                                 <sld:LineSymbolizer enabled="true">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-breaks">
                                          <ogc:PropertyName>lineData/segmentSize</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>                                       
                                    </sld:Geometry>
                                    <sld:Stroke>
                                       <sld:CssParameter name="stroke">#FF0000</sld:CssParameter>
                                       <sld:CssParameter name="stroke-width">5</sld:CssParameter>
                                    </sld:Stroke>
                                 </sld:LineSymbolizer>
                              </style>
                              <style>
                                 <sld:LineSymbolizer enabled="true">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-breaks">
                                          <ogc:PropertyName>lineData/segmentSize</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>                                       
                                    </sld:Geometry>
                                    <sld:Stroke>
                                       <sld:CssParameter name="stroke">#FFFF00</sld:CssParameter>
                                       <sld:CssParameter name="stroke-width">1</sld:CssParameter>
                                    </sld:Stroke>
                                 </sld:LineSymbolizer>
                              </style>
                              <style>
                                 <sld:PointSymbolizer enabled="false">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>                                       
                                    </sld:Geometry>
                                    <sld:Graphic>
                                       <sld:Mark>
                                          <sld:WellKnownName>circle</sld:WellKnownName>
                                          <sld:Fill>
                                             <sld:CssParameter name="fill">#000000</sld:CssParameter>
                                          </sld:Fill>
                                       </sld:Mark>
                                       <sld:Size>3</sld:Size>
                                    </sld:Graphic>
                                 </sld:PointSymbolizer>
                              </style>  
                              <style>
                                 <sld:TextSymbolizer enabled="true">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>                                       
                                    </sld:Geometry>
                                    <sld:Label>
                                       <ogc:PropertyName>lineData/roadName</ogc:PropertyName>
                                    </sld:Label>
                                    <sld:Fill>
                                       <sld:CssParameter name="fill">#CC6600</sld:CssParameter>
                                    </sld:Fill>
                                 </sld:TextSymbolizer>
                              </style>
                           </DataItem>
                        </member>
                        <member>
                           <DataItem enabled="false">
                              <name>AL Highways</name>
                              <dataProvider xlink:href="#AL_HIGHWAYS"/>
                              <style>
                                 <sld:LineSymbolizer enabled="true">
                                    <sld:Geometry>
                                       <sld:CssParameter name="geometry-breaks">
                                          <ogc:PropertyName>lineData/segmentSize</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-x">
                                          <ogc:PropertyName>lineData/segment/point/lon</ogc:PropertyName>
                                       </sld:CssParameter>
                                       <sld:CssParameter name="geometry-y">
                                          <ogc:PropertyName>lineData/segment/point/lat</ogc:PropertyName>
                                       </sld:CssParameter>                                       
                                    </sld:Geometry>
                                    <sld:Stroke>
                                       <sld:CssParameter name="stroke">#0000FF</sld:CssParameter>
                                       <sld:CssParameter name="stroke-width">2</sld:CssParameter>
                                    </sld:Stroke>
                                 </sld:LineSymbolizer>
                              </style>
                           </DataItem>
                        </member>
                     </DataList>
                  </member>
               </DataList>
            </contents>
         </Scene>
      </member>
   </SceneList>
