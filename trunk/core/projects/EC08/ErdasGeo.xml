               <!--~~~~~~~~~~~~~~~~~~~~~~~-->
               <!--transformation from airplane to nadir plane view vector  -->
               <!--~~~~~~~~~~~~~~~~~~~~~~~-->
               <sml:component name="VectorMatrixMultiplierToNadirPlane">
                  <sml:ProcessModel>
                     <sml:inputs>
