<?xml version="1.0" encoding="UTF-8"?>
<STTProject xmlns="http://vast.uah.edu/stt/v3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml"
   xmlns:ogc="http://www.opengis.net/ogc" xmlns:sld="http://www.opengis.net/sld"
   xmlns:swe="http://www.opengis.net/swe">
   <DataList id="GAMMA">
      <name>Gamma Radiation Detector</name>
      <member visible="false">
         <DataItem>
            <name>Gamma Detector Real-Time Alert</name>
            <dataProvider>
               <SensorMLProvider>
                  <name>Gamma SAS XMPP Process Chain</name>
                  <timeExtent>
                     <TimeExtent>
                        <baseTime>now</baseTime>
                        <lagTime>0</lagTime>
                        <leadTime>300</leadTime>
                        <stepTime>0</stepTime>
                     </TimeExtent>
                  </timeExtent>
                  <process xlink:href="./GammaXMPP.xml#GAMMA_XMPP"/>
               </SensorMLProvider>
            </dataProvider>
            <style>
               <sld:PointSymbolizer enabled="true">
                  <sld:Geometry>
                     <sld:CssParameter name="geometry-x">
                        <ogc:PropertyName>alert/longitude</ogc:PropertyName>
                     </sld:CssParameter>
                     <sld:CssParameter name="geometry-y">
                        <ogc:PropertyName>alert/latitude</ogc:PropertyName>
                     </sld:CssParameter>
                     <sld:CssParameter name="geometry-z">
                        <ogc:PropertyName>alert/altitude</ogc:PropertyName>
                     </sld:CssParameter>
                  </sld:Geometry>
                  <sld:Graphic>
                     <sld:ExternalGraphic>
                        <sld:Format>image/png</sld:Format>
                        <sld:OnlineResource>
                           <CategoryLookUpTable>
                              <ogc:PropertyName>alert/AlertType</ogc:PropertyName>
                              <Mapping key="HI_ALARM">icons/radioactiveAlertrrr.png</Mapping>
                              <Default>icons/radioactiveNormal2.tif</Default>
                           </CategoryLookUpTable>
                        </sld:OnlineResource>
                     </sld:ExternalGraphic>
                     <sld:Size>
                        <CategoryLookUpTable>
                           <ogc:PropertyName>alert/AlertType</ogc:PropertyName>
                           <Mapping key="HI_ALARM">0.7</Mapping>
                           <Default>0.7</Default>
                        </CategoryLookUpTable>
                     </sld:Size>
                     <sld:Spacing>100</sld:Spacing>
                  </sld:Graphic>
               </sld:PointSymbolizer>
            </style>
     <!--        <style>
               <sld:TextSymbolizer enabled="true">
                  <sld:Geometry>
                     <sld:CssParameter name="geometry-x">
                        <ogc:PropertyName>alert/longitude</ogc:PropertyName>
                     </sld:CssParameter>
                     <sld:CssParameter name="geometry-y">
                        <ogc:PropertyName>alert/latitude</ogc:PropertyName>
                     </sld:CssParameter>
                     <sld:CssParameter name="geometry-z">
                        <ogc:PropertyName>alert/altitude</ogc:PropertyName>
                     </sld:CssParameter>
                  </sld:Geometry>
                  <sld:Label>
                     <ogc:PropertyName>toDisplay</ogc:PropertyName>
                  </sld:Label>
                  <sld:Fill>
                     <sld:CssParameter name="fill">#FFFFFF</sld:CssParameter>
                  </sld:Fill>
               </sld:TextSymbolizer>
            </style>  -->
         </DataItem>
      </member>
      <member>
         <ChartScene>
            <name>Gamma Observation Chart</name>
           <domainAxis>
               <name></name>
               <Range>
                  <min>1234548000</min>
                  <max>1234551400</max>
               </Range>
            </domainAxis>
            <rangeAxis>
               <name>Radiation Count</name>
               <Range>
                  <min>1000</min>
                  <max>1100</max>
               </Range>
            </rangeAxis>
            <contents>
               <DataList>
                  <member visible="false">
                     <DataItem>
                        <name>Gamma SOS Data</name>
                        <dataProvider>
                           <SensorMLProvider>
                              <name>Gamma SOS</name>
                              <timeExtent>
                                 <TimeExtent>
                                     <baseTime>2009-02-13T18:00:00Z</baseTime>
<!--                                    <baseTime>now</baseTime>-->
                                    <leadTime>3600</leadTime>
                                    <lagTime>0</lagTime>
                                    <stepTime>0</stepTime>
                                 </TimeExtent>
                              </timeExtent>
                              <process xlink:href="./SOSGAMMA.xml#GAMMA"/>
                           </SensorMLProvider>
                        </dataProvider>
                        <style>
                           <sld:LineSymbolizer enabled="true">
                              <sld:Geometry>
                                 <sld:CssParameter name="geometry-breaks">
                                    <ogc:PropertyName>/</ogc:PropertyName>
                                 </sld:CssParameter>
                                 <sld:CssParameter name="geometry-x">
                                    <ogc:PropertyName>data/time</ogc:PropertyName>
                                 </sld:CssParameter>
                                 <sld:CssParameter name="geometry-y">
                                    <ogc:PropertyName>data/averageRadiation</ogc:PropertyName>
                                 </sld:CssParameter>                                       
                              </sld:Geometry>
                              <sld:Stroke>
                                 <sld:CssParameter name="stroke">#FF0000</sld:CssParameter>
                                 <sld:CssParameter name="stroke-width">1</sld:CssParameter>
                              </sld:Stroke>
                           </sld:LineSymbolizer>
                        </style>                               
                     </DataItem>
                  </member>                                    
               </DataList>
            </contents>
         </ChartScene>
      </member>
   </DataList>
</STTProject>
