<?xml version="1.0" encoding="UTF-8"?>
<sml:SensorML xmlns:gml="http://www.opengis.net/gml" xmlns:sml="http://www.opengis.net/sensorML/1.0" xmlns:swe="http://www.opengis.net/swe/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/sensorML/1.0 file:///D:/Sensia/Projects/SensorML/Schema/sensorML/sensorML.xsd" version="1.0">
   <sml:member>
      <sml:ProcessChain gml:id="FRAME_PROCESS">
         <sml:inputs>
            <sml:InputList>
               <sml:input name="time">
                  <swe:DataRecord gml:id="INPUT_TIME">
                     <swe:field name="start">
                        <swe:Time definition="urn:ogc:def:phenomenon:time" referenceFrame="urn:ogc:def:crs:julianTime">
                           <swe:uom code="s"/>
                        </swe:Time>
                     </swe:field>
                     <swe:field name="stop">
                        <swe:Time definition="urn:ogc:def:phenomenon:time" referenceFrame="urn:ogc:def:crs:julianTime">
                           <swe:uom code="s"/>
                        </swe:Time>
                     </swe:field>
                     <swe:field name="step">
                        <swe:Quantity definition="urn:ogc:def:phenomenon:duration">
                           <swe:uom code="s"/>
                        </swe:Quantity>
                     </swe:field>
                  </swe:DataRecord>
               </sml:input>
            </sml:InputList>
         </sml:inputs>
         <sml:outputs>
            <sml:OutputList>
               <sml:output name="grid">
                  <swe:DataArray>
                     <swe:elementCount>
                        <swe:Count>
                           <swe:value>2</swe:value>
                        </swe:Count>
                     </swe:elementCount>
                     <swe:elementType name="acrossTrackPoints">
                        <swe:DataArray>
                           <swe:elementCount>
                              <swe:Count>
                                 <swe:value>2</swe:value>
                              </swe:Count>
                           </swe:elementCount>
                           <swe:elementType name="groundPoint">
                              <swe:Vector definition="urn:ogc:def:data:locationVector" referenceFrame="urn:ogc:def:crs:ECEF_wgs84">
                                 <swe:coordinate name="x">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="y">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="z">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                              </swe:Vector>
                           </swe:elementType>
                        </swe:DataArray>
                     </swe:elementType>
                   </swe:DataArray>
               </sml:output>
               <sml:output name="frameData" xlink:href="#DATA"/>
            </sml:OutputList>
         </sml:outputs>
         <sml:components>
            <sml:ComponentList>
            <sml:component name="videoSos">
                  <sml:ProcessModel>
                     <sml:inputs>
                        <sml:InputList>
                           <sml:input name="time" xlink:href="#INPUT_TIME"/>
                        </sml:InputList>
                     </sml:inputs>
                     <sml:outputs>
                        <sml:OutputList>
                           <sml:output name="observationInfo">
                              <swe:DataRecord>
                                 <swe:field name="name">
                                    <swe:Category/>
                                 </swe:field>
                                 <swe:field name="procedure">
                                    <swe:Category/>
                                 </swe:field>
                                 <swe:field name="location">
                                    <swe:Vector referenceFrame="urn:ogc:def:crs:EPSG:6.1:4329">
                                       <swe:coordinate name="lat">
                                          <swe:Quantity/>
                                       </swe:coordinate>
                                       <swe:coordinate name="lon">
                                          <swe:Quantity/>
                                       </swe:coordinate>
                                       <swe:coordinate name="alt">
                                          <swe:Quantity/>
                                       </swe:coordinate>
                                    </swe:Vector>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:output>
                           <sml:output name="observationData">
     						  <swe:DataRecord gml:id="DATA" definition="urn:ogc:def:property:OGC::video">
      						   <swe:field name="SamplingTime">
        						  <swe:Time referenceTime="1970-01-01T00:00:00.000Z">
         						    <swe:uom code="ms"/>
         						  </swe:Time>
        					   </swe:field>
      						   <swe:field name="ImageByteLength">
      						     <swe:Count definition="urn:ogc:def:property:OGC:image:byteLength"/>
       						   </swe:field>
        					   <swe:field name="numRows">
        						  <swe:Count gml:id="nRows" definition="urn:ogc:def:property:OGC:numberOfRow"/>
       						   </swe:field>
       						   <swe:field name="numCols">
     						     <swe:Count gml:id="nCols" definition="urn:ogc:def:property:OGC:numberOfColumn"/>
       						   </swe:field>
       						   <swe:field name="Image">
        						  <swe:DataArray gml:id="VIDEO_FRAME" definition="urn:ogc:def:data:OGC:image:row">
            						  <swe:elementCount ref="nRows"/>
            						  <swe:elementType name="scanLine">
              							<swe:DataArray definition="urn:ogc:def:data:OGC:image:column">
                						  <swe:elementCount ref="nCols"/>
                						  <swe:elementType name="pixel">
                  							<swe:SimpleDataRecord definition="urn:ogc:def:data:OGC:image:pixel">
                    						  <swe:field name="red">
                      							<swe:Quantity definition="urn:ogc:def:property:OGC:radianceRed">
                        							<gml:description>Radiance measured on band1 and assigned to red channel</gml:description>
                      							</swe:Quantity>
                    						  </swe:field>
                    						  <swe:field name="green">
                      							<swe:Quantity definition="urn:ogc:def:property:OGC:radianceGreen">
                        						    <gml:description>Radiance measured on band2 and assigned to green channel</gml:description>
                      							</swe:Quantity>
                    						  </swe:field>
                    						  <swe:field name="blue">
                      							<swe:Quantity definition="urn:ogc:def:property:OGC:radianceBlue">
                        							<gml:description>Radiance measured on band3 and assigned to blue channel</gml:description>
                      							</swe:Quantity>
                    						  </swe:field>
                  							</swe:SimpleDataRecord>
                						  </swe:elementType>
              							</swe:DataArray>
           							  </swe:elementType>
          							</swe:DataArray>
      						      </swe:field>
   			  			        </swe:DataRecord>
                           </sml:output>
                        </sml:OutputList>
                     </sml:outputs>
                     <sml:parameters>
                        <sml:ParameterList>
                           <sml:parameter name="sosOptions">
                              <swe:DataRecord fixed="true">
                                 <swe:field name="endPoint">
                                    <swe:Category definition="urn:ogc:def:identifier:URL">
                                       <swe:value>http://www.botts-inc.com:8080/IED_VID/sos</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="requestMethod">
                                    <swe:Category definition="urn:ogc:def:identifier:URL">
                                       <swe:value>POST</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="version">
                                    <swe:Category definition="urn:ogc:def:identifier:version">
                                       <swe:value>1.0.0</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="offering">
                                    <swe:Category>
                                       <swe:value>axiscam.video</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="procedures">
                                    <swe:Category>
                                       <swe:value>urn:ogc:object:sensor:BOTTS-INC:bottsCam0</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="observables">
                                    <swe:Category>
                                       <swe:value>urn:ogc:def:property:OGC:radiance</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="format">
                                    <swe:Category definition="urn:ogc:def:identifier:mimeType">
                                       <swe:value>text/xml; subtype="om/1.0.0"</swe:value>
                                    </swe:Category>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:parameter>
                        </sml:ParameterList>
                     </sml:parameters>
                     <sml:method xlink:href="urn:ogc:def:process:SOS:1.0"/>
                  </sml:ProcessModel>
               </sml:component>
               <!-- SOS call to get airplane navigation data -->
               <sml:component name="sosNav">
                  <sml:ProcessModel>
                     <sml:inputs>
                        <sml:InputList>
                           <sml:input name="time" xlink:href="#INPUT_TIME"/>
                        </sml:InputList>
                     </sml:inputs>
                     <sml:outputs>
                        <sml:OutputList>
                           <sml:output name="observationInfo">
                              <swe:DataRecord>
                                 <swe:field name="name">
                                    <swe:Category/>
                                 </swe:field>
                                 <swe:field name="procedure">
                                    <swe:Category/>
                                 </swe:field>
                                 <swe:field name="location">
                                    <swe:Vector referenceFrame="urn:ogc:def:crs:EPSG:6.1:4329">
                                       <swe:coordinate name="lat">
                                          <swe:Quantity/>
                                       </swe:coordinate>
                                       <swe:coordinate name="lon">
                                          <swe:Quantity/>
                                       </swe:coordinate>
                                       <swe:coordinate name="alt">
                                          <swe:Quantity/>
                                       </swe:coordinate>
                                    </swe:Vector>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:output>
                           <sml:output name="observationData">
                              <swe:DataRecord gml:id="NAV_DATA">
			                     <swe:field name="time">
                        			<swe:Time definition="urn:ogc:phenomenon:time:iso8601"/>
                     			 </swe:field>
                     			 <swe:field name="latitude">
                        			<swe:Quantity definition="urn:ogc:phenomenon:latitude:wgs84">
                           			  <swe:uom code="deg"/>
                        			</swe:Quantity>
                     			 </swe:field>
                     			 <swe:field name="longitude">
                        			<swe:Quantity definition="urn:ogc:phenomenon:longitude:wgs84">
                           			  <swe:uom code="deg"/>
                        			</swe:Quantity> 
                     			 </swe:field>
                     			 <swe:field name="trueHeading">
                        			<swe:Quantity definition="urn:ogc:phenomenon:angle">
                           			  <swe:uom code="deg"/>
                        			</swe:Quantity> 
                     			 </swe:field>
                     			 <swe:field name="pitch">
                        			<swe:Quantity definition="urn:ogc:phenomenon:angle">
                           			  <swe:uom code="deg"/>
                        			</swe:Quantity>
                     			 </swe:field>
                     			 <swe:field name="roll">
                        			<swe:Quantity definition="urn:ogc:phenomenon:angle">
                           			  <swe:uom code="deg"/>
                        			</swe:Quantity> 
                     			 </swe:field>
                     			 <swe:field name="derivedTH">
                        			<swe:Quantity definition="urn:ogc:phenomenon:angle">
                           			  <swe:uom code="deg"/>
                        			</swe:Quantity> 
                     			 </swe:field>
                     			 <swe:field name="groundElevation">
                        			<swe:Quantity definition="urn:ogc:phenomenon:angle">
                           				<swe:uom code="m"/>
                        			</swe:Quantity>
                     			 </swe:field>
                     			 <swe:field name="derivedPitch">
                       				 <swe:Quantity definition="urn:ogc:phenomenon:angle">
                           				<swe:uom code="deg"/>
                        			 </swe:Quantity>
                     		     </swe:field>
                  			  </swe:DataRecord>
                           </sml:output>
                        </sml:OutputList>
                     </sml:outputs>
                     <sml:parameters>
                        <sml:ParameterList>
                           <sml:parameter name="sosOptions">
                              <swe:DataRecord fixed="true">
                                 <swe:field name="endPoint">
                                    <swe:Category definition="urn:ogc:def:identifier:URL">
                                       <swe:value>http://localhost:8080/OWS7/IED_VID</swe:value>
                                       <!-- <swe:value>http://sosdemos.appspot.com/tigershark</swe:value> -->
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="requestMethod">
                                    <swe:Category definition="urn:ogc:def:identifier:URL">
                                       <swe:value>GET</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="version">
                                    <swe:Category definition="urn:ogc:def:identifier:version">
                                       <swe:value>1.0</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="offering">
                                    <swe:Category>
                                       <swe:value>CAR_NAV</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="observables">
                                    <swe:Category>
                                       <swe:value>NAVIGATION</swe:value>
                                    </swe:Category>
                                 </swe:field>
                                 <swe:field name="format">
                                    <swe:Category definition="urn:ogc:def:identifier:mimeType">
                                       <swe:value>text/xml; subtype="om/1.0"</swe:value>
                                    </swe:Category>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:parameter>
                        </sml:ParameterList>
                     </sml:parameters>
                     <sml:method xlink:href="urn:ogc:def:process:SOS:1.0"/>
                  </sml:ProcessModel>
               </sml:component>
               <!-- FOR loop for along scan steps -->
               <sml:component name="asCounter">
                  <sml:ProcessModel>
                     <sml:inputs>
                        <sml:InputList>
                           <sml:input name="pass-through">
                              <swe:DataRecord>
                                 <swe:field name="nav" xlink:href="#NAV_DATA"/>
                              </swe:DataRecord>
                           </sml:input>
                           <sml:input name="countingRange">
                              <swe:DataRecord>
                                 <swe:field name="start">
                                    <swe:Quantity>
                                       <swe:value>1</swe:value>
                                    </swe:Quantity>
                                 </swe:field>
                                 <swe:field name="stop">
                                    <swe:Quantity>
                                       <swe:value>704</swe:value>
                                    </swe:Quantity>
                                 </swe:field>
                                 <swe:field name="stepCount">
                                    <swe:Quantity>
                                       <swe:value>2</swe:value>
                                    </swe:Quantity>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:input>
                        </sml:InputList>
                     </sml:inputs>
                     <sml:outputs>
                        <sml:OutputList>
                           <sml:output name="pass-through">
                              <swe:DataRecord>
                                 <swe:field name="nav" xlink:href="#NAV_DATA"/>
                              </swe:DataRecord>
                           </sml:output>
                           <sml:output name="count">
                              <swe:DataRecord>
                                 <swe:field name="variable">
                                    <swe:Quantity gml:id="AS_PIXEL"/>
                                 </swe:field>
                                 <swe:field name="index">
                                    <swe:Count/>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:output>
                           <sml:output name="stepCount">
                              <swe:Count/>
                           </sml:output>
                        </sml:OutputList>
                     </sml:outputs>
                     <sml:method xlink:href="urn:ogc:def:process:counter:1.0"/>
                  </sml:ProcessModel>
               </sml:component>
               <!-- FOR loop for across track steps -->
               <sml:component name="atCounter">
                  <sml:ProcessModel>
                     <sml:inputs>
                        <sml:InputList>
                           <sml:input name="pass-through">
                              <swe:DataRecord>
                                 <swe:field name="nav" xlink:href="#NAV_DATA"/>
                                 <swe:field name="Along_scan_pixel" xlink:href="#AS_PIXEL"/>
                              </swe:DataRecord>
                           </sml:input>
                           <sml:input name="countingRange">
                              <swe:DataRecord>
                                 <swe:field name="start">
                                    <swe:Quantity>
                                       <swe:value>1</swe:value>
                                    </swe:Quantity>
                                 </swe:field>
                                 <swe:field name="stop">
                                    <swe:Quantity>
                                       <swe:value>480</swe:value>
                                    </swe:Quantity>
                                 </swe:field>
                                 <swe:field name="stepCount">
                                    <swe:Quantity>
                                       <swe:value>2</swe:value>
                                    </swe:Quantity>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:input>
                        </sml:InputList>
                     </sml:inputs>
                     <sml:outputs>
                        <sml:OutputList>
                           <sml:output name="pass-through">
                              <swe:DataRecord>
                                 <swe:field name="nav" xlink:href="#NAV_DATA"/>
                                 <swe:field name="Along_scan_pixel" xlink:href="#AS_PIXEL"/>
                              </swe:DataRecord>
                           </sml:output>
                           <sml:output name="count">
                              <swe:DataRecord>
                                 <swe:field name="variable">
                                    <swe:Quantity/>
                                 </swe:field>
                                 <swe:field name="index">
                                    <swe:Count/>
                                 </swe:field>
                              </swe:DataRecord>
                           </sml:output>
                           <sml:output name="stepCount">
                              <swe:Count/>
                           </sml:output>
                        </sml:OutputList>
                     </sml:outputs>
                     <sml:method xlink:href="urn:ogc:def:process:counter:1.0"/>
                  </sml:ProcessModel>
               </sml:component>
               <!-- Actual chain doing the geolocation job -->
               <sml:component name="lookVectorChain" xlink:href="./AxisCarGeolocation.xml#GEOLOCATION"/>
               <!-- array reconstruction -->
               <!-- Intersection w/ Ellipsoid -->
               <sml:component name="rayIntersectEllipsoid">
                  <sml:ProcessModel>
                     <sml:inputs>
                        <sml:InputList>
                           <sml:input name="rayOrigin">
                              <swe:Vector definition="urn:ogc:def:data:locationVector">
                                 <swe:coordinate name="x">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="y">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="z">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                              </swe:Vector>
                           </sml:input>
                           <sml:input name="rayDirection">
                              <swe:Vector definition="urn:ogc:def:data:directionVector">
                                 <swe:coordinate name="x">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="y">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="z">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                              </swe:Vector>
                           </sml:input>
                           <sml:input name="heightAboveEllipsoid">
                              <swe:Quantity>
                                 <swe:uom code="m"/>
                              </swe:Quantity>
                           </sml:input>
                        </sml:InputList>
                     </sml:inputs>
                     <sml:outputs>
                        <sml:OutputList>
                           <sml:output name="intersection">
                              <swe:Vector definition="urn:ogc:def:data:locationVector">
                                 <swe:coordinate name="x">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="y">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="z">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                              </swe:Vector>
                           </sml:output>
                        </sml:OutputList>
                     </sml:outputs>
                     <sml:method xlink:href="urn:ogc:def:process:rayIntersectEllipsoid:1.1"/>
                  </sml:ProcessModel>
               </sml:component>
               <sml:component name="gridBuilder">
                  <sml:ProcessModel>
                     <sml:inputs>
                        <sml:InputList>
                           <sml:input name="groundPoint">
                              <swe:Vector definition="urn:ogc:def:data:locationVector" referenceFrame="urn:ogc:def:crs:ECEF_wgs84">
                                 <swe:coordinate name="x">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="y">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                                 <swe:coordinate name="z">
                                    <swe:Quantity>
                                       <swe:uom code="m"/>
                                    </swe:Quantity>
                                 </swe:coordinate>
                              </swe:Vector>
                           </sml:input>                         
                        </sml:InputList>
                     </sml:inputs>
                     <sml:outputs>
                        <sml:OutputList>
                           <sml:output name="grid">
                              <swe:DataArray>
                                 <swe:elementCount>
                                    <swe:Count>
                                       <swe:value>2</swe:value>
                                    </swe:Count>
                                 </swe:elementCount>
                                 <swe:elementType name="acrossTrackPoints">
                                    <swe:DataArray>
                                       <swe:elementCount>
                                          <swe:Count>
                                             <swe:value>2</swe:value>
                                          </swe:Count>
                                       </swe:elementCount>
                                       <swe:elementType name="groundPoint">
                                          <swe:Vector definition="urn:ogc:def:data:locationVector" referenceFrame="urn:ogc:def:crs:ECEF_wgs84">
                                             <swe:coordinate name="x">
                                                <swe:Quantity>
                                                   <swe:uom code="m"/>
                                                </swe:Quantity>
                                             </swe:coordinate>
                                             <swe:coordinate name="y">
                                                <swe:Quantity>
                                                   <swe:uom code="m"/>
                                                </swe:Quantity>
                                             </swe:coordinate>
                                             <swe:coordinate name="z">
                                                <swe:Quantity>
                                                   <swe:uom code="m"/>
                                                </swe:Quantity>
                                             </swe:coordinate>
                                          </swe:Vector>
                                       </swe:elementType>                                      
                                    </swe:DataArray>
                                 </swe:elementType>
                              </swe:DataArray>
                           </sml:output>
                        </sml:OutputList>
                     </sml:outputs>
                     <sml:method xlink:href="urn:ogc:def:process:gridBuilder:1.0"/>
                  </sml:ProcessModel>
               </sml:component>
            </sml:ComponentList>
         </sml:components>
         <sml:connections>
            <sml:ConnectionList>
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="this/inputs/time/"/>
                     <sml:destination ref="sosNav/inputs/time"/>
                  </sml:Link>
               </sml:connection>
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="this/inputs/time"/>
                     <sml:destination ref="videoSos/inputs/time"/>
                  </sml:Link>
               </sml:connection>
               <!-- -->
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="sosNav/outputs/observationData"/>
                     <sml:destination ref="asCounter/inputs/pass-through/nav"/>
                  </sml:Link>
               </sml:connection>
               <!-- -->
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="asCounter/outputs/pass-through/nav"/>
                     <sml:destination ref="atCounter/inputs/pass-through/nav"/>
                  </sml:Link>
               </sml:connection>
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="asCounter/outputs/count/variable"/>
                     <sml:destination ref="atCounter/inputs/pass-through/Along_scan_pixel"/>
                  </sml:Link>
               </sml:connection>
               <!-- -->
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="atCounter/outputs/pass-through/nav"/>
                     <sml:destination ref="lookVectorChain/inputs/navigationData"/>
                  </sml:Link>
               </sml:connection>
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="atCounter/outputs/pass-through/Along_scan_pixel"/>
                     <sml:destination ref="lookVectorChain/inputs/xPixel"/>
                  </sml:Link>
               </sml:connection>
               <sml:connection>
                  <sml:Link>
                     <sml:source ref="atCounter/outputs/count/variable"/>
